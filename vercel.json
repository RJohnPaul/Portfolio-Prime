	"alias": "john.dev",
	"cleanUrls": true,
	"public": true,
	"regions": ["lhr1"],
	"redirects": [
		{ "source": "/cv", "destination": "https://read.cv/john" },
		{ "source": "/github", "destination": "https://github.com/johndev" },
